{
  "name": "RSS-Reader",
  "author": "William SIMON--VEZO",
  "version": "0.0.1",
  "description": "RSS reader application. Built with electron, express, angular5 & sqlite3.",
  "keywords": [
    "electron",
    "express",
    "angular",
    "sqlite",
    "bookshelf",
    "knex"
  ],
  "license": "GPL-3.0",
  "main": "app.js",
  "scripts": {
    "ng": "ng",
    "start": "npm run build:prod & npm run electron:prod",
    "start:dev": "npm run build:dev & npm run electron:dev",
    "electron:dev": "set NODE_ENV=development & electron .",
    "electron:prod": "set NODE_ENV=production & electron .",
    "build:dev": "ng build",
    "build:prod": "ng build --prod",
    "electron_builder": "build",
    "build:sqlite3": "./node_modules/.bin/electron-rebuild -w sqlite3 -p",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test_eslint": "./node_modules/.bin/eslint ./"
  },
  "build": {
    "appId": "com.electron.rss-reader",
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {
    "@angular/animations": "^5.2.8",
    "@angular/cdk": "^5.2.4",
    "@angular/common": "^5.2.8",
    "@angular/compiler": "^5.2.8",
    "@angular/core": "^5.2.8",
    "@angular/forms": "^5.2.8",
    "@angular/http": "^5.2.8",
    "@angular/material": "^5.2.4",
    "@angular/platform-browser": "^5.2.8",
    "@angular/platform-browser-dynamic": "^5.2.8",
    "@angular/router": "^5.2.8",
    "async": "^2.6.0",
    "bookshelf": "~0.12.1",
    "core-js": "^2.4.1",
    "entities": "^1.1.1",
    "express": "~4.16.2",
    "font-awesome": "^4.7.0",
    "get-port": "^3.2.0",
    "hammerjs": "^2.0.8",
    "is-url": "~1.2.2",
    "knex": "~0.14.4",
    "lodash": "^4.17.5",
    "rxjs": "^5.5.6",
    "sqlite3": "^3.1.13",
    "striptags": "^3.1.1",
    "xml2js": "^0.4.19",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.3",
    "@angular/compiler-cli": "^5.2.8",
    "@angular/language-service": "^5.2.8",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.102",
    "ajv": "^6.2.1",
    "codelyzer": "^4.2.1",
    "electron": "1.8.3",
    "electron-builder": "~20.4.1",
    "electron-rebuild": "^1.7.3",
    "eslint": "^4.18.2",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  }
}
