{
  "name": "RSS-Reader",
  "author": "William SIMON--VEZO",
  "version": "0.0.2",
  "description": "RSS reader application. Built with electron, express4, angular6 & sqlite3.",
  "keywords": [
    "electron",
    "express",
    "angular",
    "sqlite",
    "bookshelf",
    "knex"
  ],
  "license": "GPL-3.0",
  "main": "app.js",
  "scripts": {
    "ng": "ng",
    "start": "npm run build:prod & npm run electron:prod",
    "start:dev": "npm run electron:dev",
    "electron:dev": "set NODE_ENV=development & electron .",
    "electron:prod": "set NODE_ENV=production & electron .",
    "build:dev": "ng build --watch",
    "build:prod": "ng build --prod",
    "electron_builder": "build",
    "build:sqlite3": "./node_modules/.bin/electron-rebuild -w sqlite3 -p",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test_eslint": "./node_modules/.bin/eslint ./"
  },
  "build": {
    "appId": "com.electron.rss-reader",
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {
    "@angular/animations": "6.0.2",
    "@angular/cdk": "~6.0.2",
    "@angular/common": "6.0.2",
    "@angular/compiler": "6.0.2",
    "@angular/core": "6.0.2",
    "@angular/forms": "6.0.2",
    "@angular/http": "6.0.2",
    "@angular/material": "~6.0.2",
    "@angular/platform-browser": "6.0.2",
    "@angular/platform-browser-dynamic": "6.0.2",
    "@angular/router": "6.0.2",
    "async": "~2.6.1",
    "bookshelf": "~0.12.1",
    "core-js": "~2.4.1",
    "electron-reload": "~1.2.2",
    "entities": "~1.1.1",
    "express": "~4.16.3",
    "font-awesome": "~4.7.0",
    "get-port": "~3.2.0",
    "hammerjs": "~2.0.8",
    "is-url": "~1.2.4",
    "knex": "~0.14.6",
    "lodash": "~4.17.10",
    "rxjs": "~6.1.0",
    "sqlite3": "~3.1.13",
    "striptags": "~3.1.1",
    "xml2js": "~0.4.19",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "6.0.3",
    "@angular/compiler-cli": "6.0.2",
    "@angular/language-service": "6.0.2",
    "@types/jasmine": "~2.8.7",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.111",
    "ajv": "~6.2.1",
    "codelyzer": "~4.2.1",
    "electron": "1.8.3",
    "electron-builder": "~20.4.1",
    "electron-rebuild": "~1.7.3",
    "eslint": "~4.18.2",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.3",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "~0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "2.7.2",
    "@angular-devkit/build-angular": "~0.6.3"
  }
}
